<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="hu.mapper.AdminMapper">

    <select id="list" resultType="hu.pojo.Admin">
        select * from admin
        <where>
            <if test="username != null and username != ''">username = #{username}</if>
            <if test="adminId != null">and admin_id = #{adminId}</if>
            <if test="realName != null and realName != ''">and real_name like concat('%',#{realName},'%')</if>
            <if test="adminType != null">and admin_type = #{adminType}</if>
            <if test="startTime != null and endTime != null">and last_login between #{startTime} and #{endTime}</if>
        </where>
    </select>
</mapper>